public class ChallengeOne {

	integer i;
	string s;
	Account a;
    
    public ChallengeOne(){
        
    }
	
    public void SetValues(){
       i=3;
       s='string';
       a = new Account();
       a.Name = 'Bob';  
    }   
    
    public void GetValues(){
        system.debug(i + s + a);
    }
    
    
}
